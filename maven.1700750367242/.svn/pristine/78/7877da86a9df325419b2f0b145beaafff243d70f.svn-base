@import url("https://fonts.googleapis.com/css2?family=Varela+Round&display=swap");
 html, body {
	 position: relative;
	 height: 100%;
	 font-family: "Varela Round", sans-serif;
	 width: 100%;
	 padding: 0;
	 margin: 0;
	 background:#000 !important;
	 overflow: hidden;
}
 :after, :before {
	 -webkit-box-sizing: border-box;
	 -moz-box-sizing: border-box;
	 box-sizing: border-box;
}
 * {
	 -webkit-box-sizing: border-box;
	 -moz-box-sizing: border-box;
	 box-sizing: border-box;
	 font-family: "Varela Round", sans-serif;
}
 h1, h2, h3, h4, h5, h5 {
	 font-weight: 600;
}
 section {
	 position: relative;
	 width: 100%;
}
 .header {
	 position: relative;
	 background: transparent;
	 padding: 3px 0;
}
 .header .logo img {
	 width: 300px;
}
 .copyright_con {
	 position: fixed;
	 width: 100%;
	 background: rgba(8, 7, 7, 0.74);
	 border-top: 1px solid #000 40;
	 padding: 15px 50px 5px;
	 margin: 0px 0 0 0;
	 bottom: 0;
}
 .copyright_con p {
	 margin: -10px 0 -2px 0;
	 font-weight: 400;
	 color: #fff;
	 text-align: center;
	 font-size: 12px;
	 float: right;
	 border-left: 2px solid #fff 0f;
	 padding: 0 0 0 15px;
	 line-height: 30px;
}
 .copyright_con p a {
	 font-weight: 600;
	 color: #02af51;
}
 .copyright_con h3 {
	 margin: -12px 0 -12px 0;
	 float: left;
	 line-height: normal;
	 color: rgba(255, 255, 255, 0.74);
	 font-size: 12px;
	 letter-spacing: 0.5px;
}
 .copyright_con h3 img {
	 width: 120px;
	 background: #fff;
	 padding: 4px 5px 6px;
	 border-radius: 2px;
	 opacity: 0.8;
	 margin: 0 0 -10px 0;
}
 .copyright_con h3 img:hover {
	 opacity: 1;
}
 .login_content {
	 padding: 0 0 0 0;
	 position: absolute;
	 height: 275px;
	 /* background: url(../images/bgleft.png); */
	 background-size: contain;
	 bottom: 25px;
	 background-position: center;
	 background-repeat: repeat-x;
	
}
 .login_content .container {
	 position: absolute;
	 top: 0;
	 left: 0;
	 width: 100%;
	 max-width: initial;
	 height: auto;
	 margin: 0 auto;
	 padding: 0;
	 border-radius: 15px;
	 overflow: hidden;
}
 .login_content .lc_left {
	 width: calc(100% - 465px);
	 height: 100%;
	 float: left;
	 color: #333;
	 position: relative;
	 padding: 20px;
	 overflow: hidden;
}
 .login_content .lc_left:before {
	 content: "";
	 position: absolute;
	 bottom: -50%;
	 left: -50%;
	 width: 100%;
	 height: 100%;
	 background-size: cover;
	 opacity: 0.02;
	 transition: 0.3s all;
}
 .login_content .lc_left:after {
	 content: "";
	 position: absolute;
	 top: -5%;
	 right: -66%;
	 width: 100%;
	 height: 100%;
	 background-size: cover;
	 opacity: 0.02;
	 transition: 0.3s all;
}
 .login_content .lc_left .lc_left_con {
	 width: 100%;
	 height: auto;
	 float: left;
	 text-align: center;
	 position: relative;
	 top: 60px;
}
 .login_content .lc_left .lc_left_con .login_contact {
	 position: absolute;
	 bottom: 0px;
	 left: 0;
	 width: 100%;
	 text-align: center;
	 font-weight: 600;
}
 .login_content .lc_left .lc_left_con .login_contact p {
	 margin: 0 0 0 0;
	 font-size: 13px;
	 border-top: 1px solid #d4d4d4;
	 padding: 5px 0 0 0;
	 color: #656565;
}
 .login_content .lc_left .lc_left_con .login_contact a {
	 display: inline-block;
	 font-size: 12px;
	 letter-spacing: 0.5px;
	 padding: 0 5px;
	 color: #6d6d6d;
}
 .login_content .lc_left img {
	 display: block;
	 width: 95px;
	 box-shadow: 0px 0px 10px #000 87;
	 padding: 6px;
	 border-radius: 8px;
	 margin: 0 auto;
	 position: relative;
	 top: -15px;
}
 .login_content .lc_left h1 {
	 position: relative;
	 text-transform: uppercase;
	 font-weight: 500;
	 color: #e62c1e;
	 text-shadow: 2px 2px 2px #000 57;
	 margin: 5px 0 2px 0;
	 font-size: 44px;
	 line-height: 34px;
	 text-align: left;
	 font-family: "Secular One", sans-serif;
}
 .login_content .lc_left h2 {
	 font-size: 16px;
	 margin: 0 0 0 0;
	 color: #6c65aa;
}
 .login_content:hover .lc_left:before {
	 bottom: -60%;
	 left: -60%;
}
 .login_content:hover .lc_left:after {
	 right: -90%;
}
 .login_content .lc_right {
	 position: absolute;
	 top: 11%;
	 left: 5%;
	 width: 400px;
	 padding: 10px 20px 10px 20px;
	 height: 460px;
	 float: left;
	 box-shadow: -1px 0px 10px #000 3d;
	 border-radius: 10px;
}
 .login_content .lc_right .lc_right_con {
	 width: 100%;
	 float: left;
	 position: relative;
}
 .login_content .lc_right .lc_right_con h2 {
	 color: #4a587b;
	 font-size: 28px;
	 font-weight: 600;
	 padding-top: 6%;
	 margin-left: -15px;
}
 .login_content .lc_right .lc_right_con > p {
	 position: absolute;
	 bottom: -90px;
	 width: 100%;
	 text-align: center;
	 color: #fff;
	 font-weight: 500;
	 font-size: 14px;
}
 .login_content .lc_right .lc_right_con > p > a {
	 color: #20c325;
	 font-weight: 500;
}
 .login_content .lc_right .lc_right_con .form-footer label {
	 font-size: 14px;
	 font-weight: 600;
	 letter-spacing: normal;
	 display: inline-block;
}
 .login_content .lc_right .lc_right_con .form-footer label a {
	 color: #2c2c2c !important;
	 font-weight: 500;
}
 .logotext {
	 padding-left: 0 !important;
}
 .logotext .highlight {
	 color: #ef5134;
}
 .logotext h5 {
	 text-align: left;
	 font-weight: 600;
	 font-size: 15px;
	 color: #0e0e0e;
	 line-height: 25px;
}
 .logoimg {
	 padding-right: 0 !important;
}
 label {
	 display: block;
	 letter-spacing: 0.7px;
	 color: #2c2c2c;
	 text-shadow: 1px 1px 1px #000 24;
	 font-weight: 600;
	 margin: 0 0 2px 0;
	 font-size: 15px;
}
 #btn_submit {
	 display: block;
	 font-size: 20px;
	 background: #f53400;
	 width: 100%;
	 height: auto;
	 line-height: 32px;
	 border-radius: 2px;
	 box-shadow: 0px 5px 5px #000 63;
	 color: #fff;
	 border: 0;
	 text-transform: uppercase;
	 font-weight: 600;
	 transition: 0.2s all;
	 padding: 5px 0;
	 border-radius: 5px;
}
 #btn_submit:hover, #btn_submit:focus {
	 cursor: pointer;
	 background: #212120;
	 box-shadow: 0px 8px 8px #000 59;
}
 .form-control {
	 font-weight: 600;
	 color: #131313;
	 display: block;
	 width: 100%;
	 height: 38px !important;
	 padding: 6px 12px;
	 font-size: 14px;
	 line-height: 2.42857143 !important;
	 background-image: none;
	 border: 1px solid #ccc;
	 margin: 0;
	 box-shadow: 0px 1px 4px rgba(0, 0, 0, 0.12) !important;
	 border-radius: 5px !important;
}
 .footer-link {
	 margin-top: 10px;
	 padding-left: 35px;
}
 .footer-link label a {
	 color: #ef5134;
}
 .naveenarea {
	 padding-left: 0 !important;
}
 .naveenarea img {
	 width: 100%;
}
 .form-group {
	 line-height: 2 !important;
	 line-height: 2.428571 !important;
}
 .btm-footer {
	 margin-top: 10%;
	 text-align: center;
}
 .btm-footer label {
	 font-weight: 400;
}
 .btm-footer label span a {
	 color: #e62c1e;
	 font-weight: 600;
}
 .col, .col-1, .col-10, .col-11, .col-12, .col-2, .col-3, .col-4, .col-5, .col-6, .col-7, .col-8, .col-9, .col-auto, .col-lg, .col-lg-1, .col-lg-10, .col-lg-11, .col-lg-12, .col-lg-2, .col-lg-3, .col-lg-4, .col-lg-5, .col-lg-6, .col-lg-7, .col-lg-8, .col-lg-9, .col-lg-auto, .col-md, .col-md-1, .col-md-10, .col-md-11, .col-md-12, .col-md-2, .col-md-3, .col-md-4, .col-md-5, .col-md-6, .col-md-7, .col-md-8, .col-md-9, .col-md-auto, .col-sm, .col-sm-1, .col-sm-10, .col-sm-11, .col-sm-12, .col-sm-2, .col-sm-3, .col-sm-4, .col-sm-5, .col-sm-6, .col-sm-7, .col-sm-8, .col-sm-9, .col-sm-auto, .col-xl, .col-xl-1, .col-xl-10, .col-xl-11, .col-xl-12, .col-xl-2, .col-xl-3, .col-xl-4, .col-xl-5, .col-xl-6, .col-xl-7, .col-xl-8, .col-xl-9, .col-xl-auto {
	 float: left;
}
 .districtcon {
	 margin-top: -10px;
}
 .districtcon .form-group .col-sm-12 {
	 padding: 0 !important;
}
 .vdo_content {
	 position: fixed;
	 bottom: 0;
	 background: rgba(0, 0, 0, 0.5);
	 color: #f1f1f1;
	 width: 100%;
	 padding: 20px;
}
 #myVideo {
	 position: fixed;
	 right: 0;
	 bottom: 0;
	 min-width: 100%;
	 min-height: 100%;
}
 .footertext {
	 position: absolute;
	 bottom: 0;
	 left: 34%;
	 background: #063d6f;
	 padding: 5px 12% 5px 9%;
	 color: #fff;
}
 .loginBox {
	 width: 355px;
	 padding: 10px 15px;
	 position: absolute;
	 top: 12%;
	 left: 73%;
	 background: #fff;
	 z-index: 99;
	 border-radius: 5px;
	 box-shadow: 1px 0 11px #000;
	 transform: translate(-40%, 0%);
}
 .login_con h2 {
	 font-size: 18px;
	 color: #1f1d1d;
	 padding-top: 10px;
	 font-family: sans-serif;
	 text-align: center;
}
 .upSec {
	 border-bottom: 3px solid #01679a;
}
 .upBox {
	 text-align: center;
}
 .upBox img {
	width: 337px;
}
 .form-footer {
	 padding: 0 !important;
}
 .carousel-inner img {
	 width: 100%;
	 height: auto;
	 min-width: 122%;
	 height: 100%;
}
 h3.login_lbl {
	 text-align: center;
	 padding-top: 8px;
	 font-size: 22px;
	 color: #005784;
	 font-weight: 600;
}
 .signup_modal {
	 margin: 7% 0 0 25% !important;
	 width: 30vw !important;
}
 #btn_otp {
	 font-size: 16px;
	 background: #0289c1;
	 width: 100%;
	 line-height: 28px;
	 box-shadow: 0px 5px 5px #000 63;
	 margin: 12px auto 0;
	 color: #fff;
	 border: 0;
	 text-transform: uppercase;
	 border-radius: 5px;
}
 .modal-header {
	 padding: 5px 10px !important;
	 background: #0289c1;
	 color: #fff;
}
 

/*========================================
	Responsove Media Querry
==========================================*/
@media only screen and (max-width: 900px) {
  .carousel-inner {
    height: 94vh;
  }
  .loginBox {
    transform: translate(-40%, 0%);
  }
  .carousel-inner img {
    height: 100%;
  }
  .login_content {
    display: none;
  }
}
