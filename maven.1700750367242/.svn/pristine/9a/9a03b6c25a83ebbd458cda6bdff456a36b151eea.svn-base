.swot_tbl {
    height: 340px;
}
.swot_tbl .table > thead tr{
    background: transparent;
    color: #ffffff;
}
.swot_tbl .table > thead > tr > th{
    width: 25%;
    text-transform: uppercase;
    text-align: left;
    padding: 0 3px;
    border: 0;
}
.swot_tbl .table > thead > tr > th span{
    display: block;
    padding: 10px 10px;
    background: #000;
    font-size: 14px;
    letter-spacing: 0.5px;
    border-radius: 3px 3px 0px 0px;
}
.swot_tbl .table > thead > tr > th:nth-of-type(1) span{
    background: #4CAF50;
}
.swot_tbl .table > thead > tr > th:nth-of-type(2) span{
    background: #FF9800;
}
.swot_tbl .table > thead > tr > th:nth-of-type(3) span{
    background: #009688;
}
.swot_tbl .table > thead > tr > th:nth-of-type(4) span{
    background: #FF5722;
}
.swot_tbl .table > tbody > tr > td{
    border: 0;
    text-align: left;
    padding: 0 3px;
}
.swot_tbl .swot_content{
    width: 100%;
    padding: 14px 10px 5px;
    border-bottom: 1px solid #d8d8d8;
    font-weight: 600;
    font-size: 13px;
    background: #4caf5026;
    float: left;
    min-height: 43px;
}

.swot_tbl .swot_content span:nth-of-type(1){
    float: left;
    display: block;
    width: calc(100% - 50px);
    line-height: 18px;
    font-size: 12px;
    color: #444444;    
}
.swot_tbl .swot_content span:nth-of-type(2){
    float: right;
    background: #4caf50;
    color: #fff;
    padding: 5px;
    border-radius: 3px;
    font-weight: 500;    
}
.swot_tbl .table > tbody > tr > td:nth-of-type(2) .swot_content{
    background: #ff980021;
}
.swot_tbl .table > tbody > tr > td:nth-of-type(3) .swot_content{
    background: #0096881a;
}
.swot_tbl .table > tbody > tr > td:nth-of-type(4) .swot_content{
    background: #ff572221;
}
.swot_tbl .table > tbody > tr > td:nth-of-type(2) .swot_content span:nth-of-type(2){
    background: #ff9800;    
}
.swot_tbl .table > tbody > tr > td:nth-of-type(3) .swot_content span:nth-of-type(2){
    background: #009688;    
}
.swot_tbl .table > tbody > tr > td:nth-of-type(4) .swot_content span:nth-of-type(2){
    background: #ff5722;    
}
