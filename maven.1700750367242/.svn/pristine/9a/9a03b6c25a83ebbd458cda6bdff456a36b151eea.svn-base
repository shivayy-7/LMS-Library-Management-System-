.single_cards{
    position: relative;
    height: auto;
    float: left;
    margin: 0 0 0px 0;
    text-align: center;
    padding: 0 0px;
    width: 100%;
}
.cards_content{
    position: relative;
    width: 100%;
    height: auto;
    min-height: 90px;
    float: left;
    border-radius: 0px;
    box-shadow: inset -4px -4px 10px rgba(0, 0, 0, 0.12);
    background: rgb(40, 139, 194);
    color: #2888be !important;
    padding: 18px 10px 10px;
}
.single_cards .card_value{
    font-size: 25px;
    font-weight: 600;
    color: #fff;
}
.single_cards .card_title{
    font-size: 12px;
    color: #ffffffc2;
    margin: 2px 0 0 0;
}
.single_cards .card_icon{
    position: absolute;
    left: 0;
    top: 0;
    padding: 10px 8px;
    font-size: 36px;
    color: rgba(255, 255, 255, 0.16);
}
.card_container.card_theme_blue .single_cards:nth-of-type(1) .cards_content{
    background: #64b5f6;
}
.card_container.card_theme_blue .single_cards:nth-of-type(2) .cards_content{
    background: #42a5f5;
}
.card_container.card_theme_blue .single_cards:nth-of-type(3) .cards_content{
    background: #2196f3;
}
.card_container.card_theme_blue .single_cards:nth-of-type(4) .cards_content{
    background: #1e88e5;
}
.card_container.card_theme_blue .single_cards:nth-of-type(5) .cards_content{
    background: #1976d2;
}
.card_container.card_theme_blue .single_cards:nth-of-type(6) .cards_content{
    background: #1565c0;
}
.card_container.card_theme_blue .single_cards:nth-of-type(7) .cards_content{
    background: #0d47a1;
}

.card_container.card_theme_orange .single_cards:nth-of-type(1) .cards_content{
